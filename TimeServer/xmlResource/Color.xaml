<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="BackgroundBrush" Color="#1f2022"/>
    <SolidColorBrush x:Key="ItemBrush" Color="#272a2b"/>
    <SolidColorBrush x:Key="HoverBrush" Color="#313234"/>
    <SolidColorBrush x:Key="InputBrush" Color="#2c2e2e"/>
    <SolidColorBrush x:Key="ButtonBrush" Color="#a6a5a1"/>
    <SolidColorBrush x:Key="ButtonHoverBrush" Color="#908f8b"/>
    <SolidColorBrush x:Key="RedBrush" Color="#d0000c"/>
    <SolidColorBrush x:Key="GreenBrush" Color="#168110"/>
    <SolidColorBrush x:Key="BlueBrush" Color="#0669bc"/>

    <Color x:Key="BackgroundColor">#1f2022</Color>
    <Color x:Key="ItemColor">#272a2b</Color>
    <Color x:Key="HoverColor">#313234</Color>
    <Color x:Key="InputColor">#2c2e2e</Color>
    <Color x:Key="ButtonColor">#a6a5a1</Color>
    <Color x:Key="ButtonHoverColor">#908f8b</Color>
    <Color x:Key="RedColor">#d0000c</Color>
    <Color x:Key="GreenColor">#168110</Color>
    <Color x:Key="BlueColor">#0669bc</Color>
    
</ResourceDictionary>